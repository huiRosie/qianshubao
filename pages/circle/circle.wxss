@import '../../component/circleList/circleList.wxss';

.container{
  background: #f2f2f2;
}

.circleTop{
  width: 100%;
  height:88rpx;
  line-height:88rpx;
  background: #fff;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #f2f2f2;
}

.circleEditImg{
  width: 42rpx;
  height: 42rpx;
  float: left;
  margin-top: 3rpx;
}

.circleMessageImg{
  width: 42rpx;
  height: 42rpx;
  float: right;
  margin-top: 3rpx;
}

.circleTitle{
  width:596rpx;
  height:48rpx;
  line-height:48rpx;
  float: left;
  font-size: 34rpx;
  text-align: center;
  color: rgb(51, 52, 64);
}

.circleBottom{
  text-align: center;
  font-size: 24rpx;
  height:40rpx;
  line-height:40rpx;
  color: rgb(153,153,153);
}